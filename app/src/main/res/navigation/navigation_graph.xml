<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/wineMainActivity4">
    <activity
        android:id="@+id/wineMainActivity4"
        android:name="com.example.getthewine.UI.Wine.WineTabs.WineMainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <activity
        android:id="@+id/signInActivity4"
        android:name="com.example.getthewine.UI.Auth.SignInActivity"
        android:label="SignInActivity" />
    <fragment
        android:id="@+id/scanLabelFragment3"
        android:name="com.example.getthewine.UI.Wine.WineTabs.ScanLabelFragment"
        android:label="fragment_page1"
        tools:layout="@layout/fragment_page1" />
    <fragment
        android:id="@+id/searchByNameFragment2"
        android:name="com.example.getthewine.UI.Wine.WineTabs.SearchByNameFragment"
        android:label="fragment_page2"
        tools:layout="@layout/fragment_page2" >
        <action
            android:id="@+id/action_searchByNameFragment2_to_wineDetailsFragment3"
            app:destination="@id/wineDetailsFragment3" />
        <argument
            android:name="wineId"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/page32"
        android:name="com.example.getthewine.UI.Wine.WineTabs.page3"
        android:label="fragment_page3"
        tools:layout="@layout/fragment_page3" />
    <fragment
        android:id="@+id/wineDetailsFragment3"
        android:name="com.example.getthewine.UI.Wine.WineTabs.WineDetailsFragment"
        android:label="wine_details_fragment"
        tools:layout="@layout/wine_details_fragment" />
